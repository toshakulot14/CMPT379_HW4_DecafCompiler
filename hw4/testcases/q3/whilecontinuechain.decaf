extern void print_int(int);
class Test {
	int main() {
		int x;
		x = 0;
		while (x < 10) {
			if (x % 2 == 0) {
				x = x + 1;
				continue;
			}
			print_int(x);
			x = x + 1;
		}
		x = 0;
		while (x < 0) {
			if (x % 2 == 0) {
				x = x + 1;
				continue;
			}
			print_int(x);
			x = x + 1;
		}
		x = 0;
		while (x < 10) {
			if (x % 2 == 1) {
				x = x + 1;
				continue;
			}
			print_int(x);
			x = x + 1;
		}
	}
}
