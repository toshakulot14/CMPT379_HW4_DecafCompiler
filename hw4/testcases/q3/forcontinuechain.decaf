extern void print_int(int);
class Test {
  int main() {
    int x;
    for(x = 0; x < 10; x = x + 1) {
      if (x % 2 == 0) {
        continue;
      }
      print_int(x);
    }
    for(x = 0; x < 0; x = x + 1) {
      if (x % 2 == 0) {
        continue;
      }
      print_int(x);
    }
    for(x = 0; x < 10; x = x + 1) {
      if (x % 2 == 1) {
        continue;
      }
      print_int(x);
    }
  }
}
