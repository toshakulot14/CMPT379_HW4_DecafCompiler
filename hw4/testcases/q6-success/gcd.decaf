
extern void print_int(int);

class GreatestCommonDivisor {

    int a = 10;
    int b = 20;
    
    int main() {
       int x, y, z;
       x = a;
       y = b;
       z = gcd(x, y);

       // print_int is part of the standard input-output library
       print_int(z); 
    }

    // function that computes the greatest common divisor
    int gcd(int a, int b) {
       if (b == 0) { return(a); } 
       else { return( gcd(b, a % b) ); }
    }

}
